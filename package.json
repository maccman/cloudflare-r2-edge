{
  "name": "cloudflare-r2-edge",
  "version": "0.0.1",
  "packageManager": "pnpm@8.9.0",
  "description": "",
  "author": "Alex MacCaw <alex@alexmaccaw.com>",
  "license": "MIT",
  "homepage": "https://github.com/maccman/cloudflare-r2-edge#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/maccman/cloudflare-r2-edge.git"
  },
  "bugs": "https://github.com/maccman/cloudflare-r2-edge/issues",
  "keywords": [],
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "fix": "eslint --fix . && prettier --write .",
    "prepublishOnly": "nr build",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.8",
    "@ocavue/eslint-config": "^1.3.1",
    "@types/node": "^20.8.7",
    "eslint": "^8.51.0",
    "esno": "^0.17.0",
    "prettier": "^3.0.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "renovate": {
    "extends": [
      "github>ocavue/config-renovate"
    ]
  },
  "dependencies": {
    "aws4fetch": "^1.0.17"
  }
}
